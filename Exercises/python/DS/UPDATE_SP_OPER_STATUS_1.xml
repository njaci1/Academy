		<!-- And the latest identity status code values can be found from the data dictionary Identity Status via SP Portal menu My Functions -> Optional Configuration -> Data Dictionary.-->
		<Status Code="06" />
	</SPOperator>
    <SPOperator UserName="KE10977" >
        <Status Code="06" />
    </SPOperator>
    <SPOperator UserName="KE16686" >
        <Status Code="06" />
    </SPOperator>
</BulkUpdateStatusForSPOperatorRequest>
